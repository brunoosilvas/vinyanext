{
  "AllowedHosts": "",
  "ConnectionStrings": {
    "PgsqlVinyanext": "",
    "PgsqlVinyanextHangfire": "",
    "MongoVinyanext": ""
  },
  "Redis": {
    "Endpoint": "",
    "User": "",
    "Password": ""
  },
  "Hangfire": {
    "Dashboard": {
      "Title": "",
      "User": "",
      "Password": ""
    }
  },
  "Jwt": {
    "Secret": "",
    "Issuer": "",
    "Audience": "",
    "ExpirationInMinutes": 0
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": [
      {
        "ClusterId": "AlmoxarifadoCluster",
        "AuthorizationPolicy": "almoxarifado",
        "Match": {
          "Path": "api-almoxarifado/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      {
        "ClusterId": "SistemaCluster",
        "AuthorizationPolicy": "sistema",
        "Match": {
          "Path": "api-sistema/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      }
    ],
    "Clusters": {
      "AlmoxarifadoCluster": {
        "Destinations": {
          "AlmoxarifadoDestination": {
            "Address": "http://localhost:5010"
          }
        }
      },
      "SistemaCluster": {
        "Destinations": {
          "SistemaDestination": {
            "Address": "http://localhost:5080"
          }
        }
      }
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://127.0.0.1:5341"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}
