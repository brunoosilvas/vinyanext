{
  "Env": "",
  "AllowedHosts": "",
  "ConnectionStrings": {
    "PgsqlVinyanext": "",
    "PgsqlVinyanextHangfire": "",
    "MongoVinyanext": ""
  },
  "Redis": {
    "Endpoint": "",
    "User": "",
    "Password": "",
    "Keys": {
      "Login": "login"
    }
  },
  "Hangfire": {
    "Dashboard": {
      "Title": "",
      "User": "",
      "Password": ""
    }
  },
  "Jwt": {
    "Secret": "",
    "Issuer": "",
    "Audience": "",
    "ExpirationInMinutes": 0
  },
  "OpenApi": {
    "Api": "http://localhost:5000/openapi/v1.json",
    "ApiSistema": "http://localhost:5080/openapi/v1.json"
  },
  "ReverseProxy": {
    "Routes": [
      {
        "ClusterId": "AlmoxarifadoCluster",
        "AuthorizationPolicy": "almoxarifado",
        "Match": {
          "Path": "/api-almoxarifado/{**catch-all}"
        },
        "Transforms": [ { "RequestHeader": "X-Api-Key", "Set": "admin@2025" } ]
      },
      {
        "ClusterId": "GestaoVoucherCluster",
        "AuthorizationPolicy": "gestao",
        "Match": {
          "Path": "/api-gestao-voucher/{**catch-all}"
        },
        "Transforms": [ { "RequestHeader": "X-Api-Key", "Set": "admin@2025" } ]
      },
      {
        "ClusterId": "GestaoVoucherPublicoCluster",
        "AuthorizationPolicy": "publico",
        "Match": {
          "Path": "/api-gestao-voucher-publico/{**catch-all}"
        },
        "Transforms": [ { "RequestHeader": "X-Api-Key", "Set": "admin@2025" } ]
      },
      {
        "ClusterId": "SistemaCluster",
        "AuthorizationPolicy": "sistema",
        "Match": {
          "Path": "/api-sistema/{**catch-all}"
        },
        "Transforms": [ { "RequestHeader": "X-Api-Key", "Set": "admin@2025" } ]
      }
    ],
    "Clusters": {
      "AlmoxarifadoCluster": {
        "Destinations": {
          "AlmoxarifadoDestination": {
            "Address": "http://localhost:5010"
          }
        }
      },
      "GestaoVoucherCluster": {
        "Destinations": {
          "GestaoVoucherDestination": {
            "Address": "http://localhost:5060"
          }
        }
      },
      "GestaoVoucherPublicoCluster": {
        "Destinations": {
          "GestaoVoucherPublicoDestination": {
            "Address": "http://localhost:5070"
          }
        }
      },
      "PessoalCluster": {
        "Destinations": {
          "PessoalDestination": {
            "Address": "http://localhost:5080"
          }
        }
      },
      "SistemaCluster": {
        "Destinations": {
          "SistemaDestination": {
            "Address": "http://localhost:5090"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://127.0.0.1:5341"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}
